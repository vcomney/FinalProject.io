      function pickBand() {
       //getting value of name input
   			var userName=document.getElementById("name").value;
        //getting the first letter of that name
        var firstLetter=userName.charAt(0);
        firstLetter=firstLetter.toLowerCase();
        //getting the lower case value
        var band=bandList[firstLetter];
        document.getElementById("results").innerHTML=band;
      };
    </script>

